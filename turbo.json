{
  "$schema": "https://turbo.build/schema.v1.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", "lib/**"]
    },
    "test:dev": {
      "interactive": true,
      "cache": false,
      "persistent": true
    },
    "test": { 
      "dependsOn": ["build"],
      "cache": false 
    },
    "test:update": { "cache": false },
    "test:unit": {"cache":false},
    "test:integration": {"cache": false},
    "test:cli": { 
      "dependsOn": ["build"],
      "cache": false 
    },
    "test:cleanup": {},
    "docs": {},
    "docker:build": {},
    "lint": {},
    "lint:fix":{"cache": false},
    "lint:tpl:validator": {},
    "test:integration:update":{"cache": false},
    "generate:assets": {
      "dependsOn": [],
      "outputs": ["assets/**"]
    }
  }
}