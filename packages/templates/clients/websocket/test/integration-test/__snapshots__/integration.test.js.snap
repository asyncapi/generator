    safeValue=$(echo \\"$value\\" | tr -d '\\\\r')
    case \\"$key\\" in wrapperUrl)
      wrapperUrl=\\"$safeValue\\"
      break
      ;;
    esac
  done <\\"$MAVEN_PROJECTBASEDIR/.mvn/wrapper/maven-wrapper.properties\\"
